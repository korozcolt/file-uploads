    "@typescript-eslint/parser": "^5.62.0",
    "jest": "^29.7.0",
    "prettier": "^2.8.8",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node-dev": "^2.0.0",
    "@types/uuid": "^9.0.2",
    "@types/jsonwebtoken": "^9.0.2",
    "typescript": "^5.2.0"
  }
