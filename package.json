  "main": "dist/server.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "ts-node-dev --transpile-only --respawn --no-notify src/server.ts",
    "prestart": "node ./scripts/ensure-dirs.js",
    "start": "node dist/server.js",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json,md}\"",
    "json": "jest --runInBand --detectOpenHandles --coverage",
    "docs": "node ./scripts/generate-openapi.js || echo 'no generator'",
    "seed-admin": "node ./scripts/seed-admin.js"
  },
  "dependencies": {
    "better-sqlite3": "^11.7.0",
    "cors": "^2.8.5",
    "dotenv": "^16.1.4",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "helmet": "^6.0.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "uuid": "^9.0.0",
    "jsonwebtoken": "^9.0.0",
    "swagger-ui-express": "^4.6.3",
    "bcryptjs": "^2.4.3"
  },

  "devDependencies": {
    "@types/better-sqlite3": "^7.6.12",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.6.1",
    "@types/supertest": "^2.0.11",
    "@types/cors": "^2.8.12",
    "@types/morgan": "^1.9.4",
